{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "target": "ES5",
    "baseUrl": "./src/",
    "outDir": "./dist/",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "es2015",
      "dom.iterable",
      "es2016.array.include",
      "es2017.object",
      "dom"
    ],
    "allowJs": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,

  }
}
